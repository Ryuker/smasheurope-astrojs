---
import { Icon } from 'astro-icon/components';
const {
  icon, 
  text, 
  url,
  classes, 
  newTab = false
} = Astro.props;

---

<a target={newTab} href={url} class={`${classes} bg-se-lite-blue hover:bg-se-darker-blue px-6 py-3 rounded-lg`}>
  <Icon name={icon} class="text-lg" />
  {text}
</a>