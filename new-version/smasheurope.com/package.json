{
  "name": "smasheurope-com",
  "type": "module",
  "version": "0.0.1",
  "homepage": "https://ryuker.github.io/smasheurope-astrojs.com",
  "standard": {
    "ignore": [
      "node_modules",
      ".astro",
      ".env"
    ]
  },
  "scripts": {
    "predeploy": "npm run build:gh-pages",
    "deploy": "gh-pages -d dist",
    "help": "gh-pages --help",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build astro build --base=/your-project-name/",
    "build:gh-pages": "astro build --base=/smasheurope-astrojs/",
    "preview": "astro preview",
    "astro": "astro",
    "server": "json-server --watch server/data/events.json --port 5000"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/tailwind": "^5.1.2",
    "@astrojs/vue": "^4.5.2",
    "@floating-ui/dom": "^1.6.12",
    "astro": "^4.16.6",
    "astro-icon": "^1.1.1",
    "json-server": "^1.0.0-beta.3",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "gh-pages": "^6.2.0"
  }
}
